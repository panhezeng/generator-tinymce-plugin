{
  "name": "@panhezeng/generator-tinymce-plugin",
  "version": "0.0.0-development",
  "description": "generator tinymce plugin",
  "keywords": [
    "tinymce plugin",
    "yeoman generator"
  ],
  "homepage": "https://github.com/panhezeng/generator-tinymce-plugin#readme",
  "bugs": {
    "url": "https://github.com/panhezeng/generator-tinymce-plugin/issues",
    "email": "panhezeng@gmail.com"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "潘何增",
    "email": "panhezeng@gmail.com",
    "url": "http://apsay.com/"
  },
  "contributors": [],
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "browser": "",
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/panhezeng/generator-tinymce-plugin.git"
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/changelog": "^3.0.6",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "semantic-release": "^15.13.31",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "peerdependencies": {},
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json,html,css,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "maintained node versions"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog"
    ]
  }
}
